{
  "name": "dota-server",
  "version": "0.0.1",
  "description": "Source code for the Delivr OTA service API",
  "main": "./script/server.js",
  "scripts": {
    "start": "node ./bin/script/server.js",
    "start:env": "node -r dotenv/config ./bin/script/server.js dotenv_config_path=.env dotenv_config_silent=true",
    "clean": "rimraf bin",
    "build": "tsc && shx cp -r ./script/views ./bin/script",
    "restart": "npm run build && npm run start:env",
    "prettier": "prettier --write \"./**/*.ts\"",
    "lint": "npx eslint ./script/**/*.ts",
    "lint:fix": "npx eslint ./script/**/*.ts --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds-horizon/delivr-server-ota"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "Microsoft",
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/identity": "^4.2.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/storage-blob": "^12.14.0",
    "@azure/storage-queue": "^12.13.0",
    "applicationinsights": "^2.7.0",
    "aws-cloudfront-sign": "^3.0.2",
    "aws-sdk": "2.1691.0",
    "blocked-at": "^1.2.0",
    "body-parser": "^1.20.2",
    "cookie-session": "^2.0.0",
    "dd-trace": "^5.36.0",
    "ejs": "^3.1.10",
    "email-validator": "1.0.3",
    "express": "^4.21.2",
    "express-domain-middleware": "0.1.0",
    "express-rate-limit": "^7.5.0",
    "google-auth-library": "9.9.0",
    "ioredis": "5.4.0",
    "lusca": "^1.7.0",
    "memcached": "2.2.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "3.11.3",
    "nanoid": "5.0.7",
    "node-deepcopy": "0.1.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.5",
    "passport-github2": "0.1.9",
    "passport-http-bearer": "1.0.1",
    "passport-windowslive": "1.0.1",
    "redis": "2.4.2",
    "sanitize-html": "^2.11.0",
    "semver": "^7.5.3",

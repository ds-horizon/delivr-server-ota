    "superagent": "^8.0.9",
    "try-json": "1.0.0",
    "wordwrap": "^1.0.0",
    "yauzl": "2.6.0",
    "yazl": "2.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.1.4",
    "@types/q": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.7",
